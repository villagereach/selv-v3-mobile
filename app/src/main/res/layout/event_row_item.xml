<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_stock_event_product6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="196dp"
        android:layout_marginTop="68dp"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_event_row_item_product" />

    <TextView
        android:id="@+id/tv_event_row_item_product"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="@string/string_product"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_event_row_item_lotcode"
        android:layout_width="83dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginTop="16dp"
        android:text="@string/string_lot"
        app:layout_constraintStart_toEndOf="@+id/tv_event_row_item_product"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_event_row_item_quantity"
        android:layout_width="83dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:text="@string/string_quantity"
        app:layout_constraintStart_toEndOf="@+id/tv_event_row_item_expiration_date"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/tv_event_row_item_expiration_date"
        android:layout_width="82dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginTop="16dp"
        android:text="@string/string_date"
        app:layout_constraintStart_toEndOf="@+id/tv_event_row_item_lotcode"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_stock_event_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_event_row_item_product" />

    <TextView
        android:id="@+id/tv_stock_event_product2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_event_row_item_product" />

    <TextView
        android:id="@+id/tv_stock_event_product3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="68dp"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_event_row_item_product" />

    <TextView
        android:id="@+id/tv_stock_event_lot"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginTop="4dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@id/tv_event_row_item_lotcode"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintStart_toEndOf="@+id/tv_stock_event_product"
        app:layout_constraintTop_toBottomOf="@+id/tv_event_row_item_lotcode" />

    <TextView
        android:id="@+id/tv_stock_event_quantity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintStart_toStartOf="@id/tv_event_row_item_expiration_date"
        app:layout_constraintTop_toBottomOf="@+id/tv_event_row_item_expiration_date"
        tools:layout_editor_absoluteX="264dp" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="81dp"
        android:layout_height="77dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_done" />

    <TextView
        android:id="@+id/label_event_row_item_reason"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/string_reason"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        tools:layout_editor_absoluteY="71dp" />

    <TextView
        android:id="@+id/label_event_row_item_source_or_destination"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="68dp" />

    <TextView
        android:id="@+id/label_event_row_item_source_or_destination2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="70dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>